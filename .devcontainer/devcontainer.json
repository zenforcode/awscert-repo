{
    "name": "Python 3.10 DevContainer",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "settings": {
      "python.defaultInterpreterPath": "/usr/local/bin/python"
    },
    "extensions": [
      "ms-python.python",
      "ms-python.vscode-pylance"
    ],
    "mounts": [ "source=/home/aws,target=/home/aws,type=bind,consistency=cached"],
    "remoteUser": "vscode"
}


